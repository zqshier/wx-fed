{
  "name": "zippo",
  "version": "3.1.1",
  "private": true,
  "description": "Zippo会员中心",
  "templateInfo": {
    "name": "vue3-NutUI4",
    "typescript": true,
    "css": "sass"
  },
  "repository": {
    "type": "git",
    "url": "https://git.dev.center.ugrow.cn/UGROW/zippo-fed.git"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "cross-env VUE_DEVTOOLS=1 npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint --ext .vue --ext .js --ext .ts src/",
    "test": "jest",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/icons-vue-taro": "^0.0.9",
    "@nutui/nutui-taro": "^4.1.6",
    "@tarojs/components": "3.6.6",
    "@tarojs/helper": "3.6.6",
    "@tarojs/plugin-framework-vue3": "3.6.6",
    "@tarojs/plugin-html": "3.6.6",
    "@tarojs/plugin-platform-weapp": "3.6.6",
    "@tarojs/runtime": "3.6.6",
    "@tarojs/shared": "3.6.6",
    "@tarojs/taro": "3.6.6",
    "date-fns": "^2.29.2",
    "lodash": "^4.17.21",
    "lottie-miniprogram": "^1.0.12",
    "lru-cache": "^7.14.0",
    "miniprogram-validator": "^1.0.0",
    "pinia": "^2.0.14",
    "taro-plugin-pinia": "^1.0.0",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@tarojs/cli": "3.6.6",
    "@tarojs/webpack5-runner": "3.6.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.11",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "babel-preset-taro": "3.6.6",
    "computeddistance": "^1.0.4",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "3.4.2",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.6.6",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.1",
    "jest": "^29.4.3",
    "lint-staged": "^13.0.3",
    "lunar-typescript": "^1.2.23",
    "miniprogram-ci": "^1.8.35",
    "prettier": "^2.7.1",
    "style-loader": "1.3.0",
    "stylelint": "9.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "unplugin-vue-components": "^0.25.2",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.39.2",
    "webpack": "^5.78.0"
  },
  "optionalDependencies": {
    "@tarojs/plugin-vue-devtools": "^3.6.8"
  }
}
